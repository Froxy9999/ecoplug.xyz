<%- include("./../../partials/head", { user, path, description, title: "Home", twitter, url, image }) %>

<title>Peter Bots | Panel</title>
<link rel="icon"
    type="image/png"
    href="<%=domain%>/logo.png">
    <body>
    <% if (alert) { %>
 <div class="container-contact">
            <div class="wrap-contact">
                
                    <span class="contact-form-title">Notice</span>
                    <h4><%= alert %></h4>
              
            </div>
        </div>
    <% } else { %>
 <div class="container-contact">
            <div class="wrap-contact">
                <form class="contact-form validate-form" role="form" action="/admin/add-bot" method="POST" enctype="multipart/form-data">
                    <span class="contact-form-title">Bots</span>
                    
          
        <% if (prods.length > 0) { %>
            <% for (let product of prods) { %>


<div class="products">
  <div class="product">
    <a href="#" class="preview">
      <img src="/<%= product.imageUrl %>" class="img-responsive">
      <p>Added by <%= product.addedBy %> and <%= product.views %> Views</p>
    </a>
    <div class="details">
      <a href="/bots/<%= product._id %>" target="_blank" class="preview name"><%= product.title %></a>
      
    </div>
    <div class="pricing">
    <p style="color: white"><%= product.price%></p>
    </div>
    <div style="text-align: center; padding-top: 5px;">
    <a href="/admin/delete/<%= product._id %>" target="_blank" style="color: white" class="delete"> Delete </a>
    </div>
  </div>
  </div>
  


           
         <% } %>
        <% } else { %>
        <h1>No Products Found!</h1>
        <% } %>
   

                </form>
              
            </div>
        </div>
<% } %>
</html>

<style>
