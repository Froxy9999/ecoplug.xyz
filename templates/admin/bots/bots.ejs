.validate-input {
    position: relative;
}

.alert-validate::before {
    top: 58%;
    right: 2px;
    opacity: 0;
    max-width: 70%;
    font-size: 13px;
    color: #c80000;
    line-height: 1.4;
    text-align: left;
    visibility: hidden;
    position: absolute;
    border-radius: 2px;
    pointer-events: none;
    background-color: #fff;
    transition: opacity .4s;
    padding: 4px 25px 4px 10px;
    transform: translateY(-50%);
    border: 1px solid #c80000;
    content: attr(data-validate);
}

